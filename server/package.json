{
  "name": "server",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register ./index.ts",
    "cron": "ts-node-dev -r tsconfig-paths/register ./cron.ts",

    "start": "pm2 start ../scripts/devOps/prod.pm2.js",

    "build": "tsc --build --clean && tsc --build --force",
    "migrate": "knex migrate:latest",

    "start:check": " npm run build && clinic clean && clinic flame -- node ./dist/index.js",
    "lint:fix": "eslint --fix ./ --ext .ts",

    "tsc": "tsc",
    "lint": "eslint ./ --ext .ts",

    "syncTickerPrices": "ts-node -r tsconfig-paths/register cli syncTickerPrices",
    "syncTickerIncomes": "ts-node -r tsconfig-paths/register cli syncTickerIncomes",
    "syncTickerEarnings": "ts-node -r tsconfig-paths/register cli syncTickerEarnings",
    "syncMonthlyIndicators": "ts-node -r tsconfig-paths/register cli syncMonthlyIndicators",
    "syncQuarterlyIndicators": "ts-node -r tsconfig-paths/register cli syncQuarterlyIndicators",
    "syncYearlyIndicators": "ts-node -r tsconfig-paths/register cli syncYearlyIndicators",
    "calcPriceMovements": "ts-node -r tsconfig-paths/register cli calcPriceMovements",
    "calcFinancialMovements": "ts-node -r tsconfig-paths/register cli calcFinancialMovements",
    "calcIndicatorMovements": "ts-node -r tsconfig-paths/register cli calcIndicatorMovements",
    "calcDailyTickers": "ts-node -r tsconfig-paths/register cli calcDailyTickers",
    "calcTraderPerformances": "ts-node -r tsconfig-paths/register cli calcTraderPerformances",
    "calcTraderDescendants": "ts-node -r tsconfig-paths/register cli calcTraderDescendants",
    "calcTraderAccessHashs": "ts-node -r tsconfig-paths/register cli calcTraderAccessHashs",
    "sendPendingEmails": "ts-node -r tsconfig-paths/register cli sendPendingEmails"
  },
  "dependencies": {
    "@shared/constants": "*",
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "ioredis": "^4.28.2",
    "ioredis-mock": "^7.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "ms": "^2.1.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.1",
    "pg-mem": "^2.3.5",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@shared/interfaces": "*",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.3",
    "@types/ioredis-mock": "^5.6.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.6",
    "@types/node-cron": "^3.0.4",
    "@types/nodemailer": "^6.4.4",
    "clinic": "^12.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0"
  }
}
