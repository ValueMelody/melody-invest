{
  "name": "dioxider-app",
  "workspaces": [
    "server"
  ],
  "version": "0.0.1",
  "description": "",
  "author": "Baozier",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byn9826/dioxider-app"
  },
  "scripts": {
    "server:start": "npm run start --workspace=server",
    "server:lint": "npm run lint --workspace=server",
    "database:create": "createdb -h 127.0.0.1 -U admin dioxider",
    "database:drop": "sudo -u postgres psql -c 'DROP DATABASE dioxider'",
    "database:dump": "pg_dump -h 127.0.0.1 -Fc dioxider -U admin > ~/Dropbox/Temp/dioxider.bak",
    "database:restore": "pg_restore -d dioxider -U admin ~/Dropbox/Temp/dioxider.bak"
  }
}